
public class IntermediateExercises {

	public static void main(String[] args) {

		System.out.println("Intermediate Exercises");
		IntermediateExercises ex = new IntermediateExercises();
		System.out.println(ex.ex1(18, 21));
		System.out.println(ex.ex1(20, 18));
		System.out.println(ex.ex1(22, 21));
		System.out.println(ex.ex1(18, 22));
		System.out.println(ex.ex1(22, 22));

	}

	int ex1(int a, int b) {

		int result = 0;
		int a2 = 0;
		int b2 = 0;
		
		if(a <= 21) {
			a2 = a;
		}
		if(b <= 21) {
			b2 = b;
		}
		if (a2 == 0 && b2 == 0)
		{
			result = 0;
		}
		if(a2 == b2)
		{
			result = a2;
		} else if (a2 > b2)
		{
			result = a2;
		} else {
			result = b2;
		}
		
		return result;
	}
}
