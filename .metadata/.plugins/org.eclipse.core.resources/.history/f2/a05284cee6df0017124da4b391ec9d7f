
public class ExceersiesOne {

	public static void main(String[] args) {

		int[] myArray = new int[100];

		for (int i = 0; i <= myArray.length; i++) {

			myArray[i] = i + 1;

			System.out.println(myArray[i]);
		}

		int input = 3;

		for (int i = 0; i <= myArray.length; i++) {

			if (myArray[i] == input) {
				i++;

			}

			System.out.println(myArray[i]);
		}

	}

}
